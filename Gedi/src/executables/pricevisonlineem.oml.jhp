	<LinesTrack input="+.<?JS print(experimentNames[e]) ?>.cod.<?JS print(names[e][i]); ?>" class="codon" id="+.<?JS print(experimentNames[e]) ?>.Codons.<?JS print(names[e][i]); ?>" >
		<LogScale base="10" />
		<FixedMin min="0"/>
