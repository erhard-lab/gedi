		<FixedMax m="1" />
		<FixedMin m="0" />
	</StackedBoxesTrack>

	<StackedBoxesTrack input="+.<?JS print(experimentNames[e]) ?>.rangescore" id="+.<?JS print(experimentNames[e]) ?>.Orfs Range score" >
		<FixedMax m="1" />
		<FixedMin m="0" />
	</StackedBoxesTrack>
	


